package class2.com.ca;

import java.util.Scanner;

public class Loop_exp3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		 int a =sc.nextInt();
		 
		 for(int i=0;i<=a;i++)
		 {
			for(int j=1;j<=i;j++)
				
			{
				if(j>=i)
				{
				System.out.print(+j);
				break;
				}
				{
				System.out.print(j +" ");
				} 
					
		
			}System.out.println();
			
			
		 }
	
			
		 }
	}

